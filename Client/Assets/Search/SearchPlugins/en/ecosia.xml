<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Ecosia</ShortName>
<InputEncoding>UTF-8</InputEncoding>
<Image width="92" height="92">data:image/png;base64,</Image>
<Url type="application/x-suggestions+json" method="GET" template=" https://www.ecosia.org/search&amp;q={searchTerms}"/>
<Url type="text/html" method="GET" template="https://www.ecosia.org/search">
  <Param name="q" value="{searchTerms}"/>
  <Param name="ie" value="utf-8"/>
  <Param name="oe" value="utf-8"/>
</Url>
<SearchForm>https://www.ecosia.org</SearchForm>
</SearchPlugin>
